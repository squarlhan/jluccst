<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<!--  月计划子项Action -->
	<package name="monthly_item_info" extends="struts-default" namespace="/">
		<action name="openMonthlyItemListAction" class="monthlyItemInfoAction" method="openMonthlyItemList">
			<result name="success">/sellreport/monthly/monthlyiteminfolist.jsp</result>
			<result name="input">/sellreport/monthly/monthlyiteminfolist.jsp</result>
		</action>
		<action name="openAddMonthlyItemAction" class="monthlyItemInfoAction" method="openAddMonthlyItem">
			<result name="success">/sellreport/monthly/monthlyiteminfoadd.jsp</result>
			<result name="input">/sellreport/monthly/monthlyiteminfoadd.jsp</result>
		</action>
		<action name="openModifyMonthlyItemAction" class="monthlyItemInfoAction" method="openModifyMonthlyItem">
			<result name="success">/sellreport/monthly/monthlyiteminfoadd.jsp</result>
			<result name="input">/sellreport/monthly/monthlyiteminfoadd.jsp</result>
		</action>
		<action name="addMonthlyItemAction" class="monthlyItemInfoAction" method="addMonthlyItem">
			<result name="success">/sellreport/monthly/monthlyiteminfoadd.jsp</result>
			<result name="input">/sellreport/monthly/monthlyiteminfoadd.jsp</result>
		</action>
		<action name="modifyMonthlyItemAction" class="monthlyItemInfoAction" method="modifyMonthlyItem">
			<result name="success">/sellreport/monthly/monthlyiteminfoadd.jsp</result>
			<result name="input">/sellreport/monthly/monthlyiteminfoadd.jsp</result>
		</action>
		<action name="deleteMonthlyItemAction" class="monthlyItemInfoAction" method="deleteMonthlyItem"/>
		
		
		
		<action name="openMonthlyItemListViewAction" class="monthlyItemInfoAction" method="openMonthlyItemListView">
			<result name="success">/sellreport/monthly/monthlyiteminfolistview.jsp</result>
			<result name="input">/sellreport/monthly/monthlyiteminfolistview.jsp</result>
		</action>
		<action name="openViewMonthlyItemAction" class="monthlyItemInfoAction" method="openViewMonthlyItem">
			<result name="success">/sellreport/monthly/monthlyiteminfoview.jsp</result>
			<result name="input">/sellreport/monthly/monthlyiteminfoview.jsp</result>
		</action>
	</package>
</struts>