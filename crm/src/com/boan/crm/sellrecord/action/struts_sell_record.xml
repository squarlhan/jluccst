<struts>
	<!--  销售记录管理Action -->
	<package name="sell_record" extends="struts-default" namespace="/">
		<action name="toTabPageAction" class="sellRecordAction" method="toTabPage">
			<result name="success">/sellrecord/sellrecordtab.jsp</result>
			<result name="input">/sellrecord/sellrecordtab.jsp</result>
		</action>
		<action name="openSellRecordListAction" class="sellRecordAction" method="openSellRecordList">
			<result name="success">/sellrecord/sellrecordlist.jsp</result>
			<result name="input">/sellrecord/sellrecordlist.jsp</result>
		</action>
		<action name="openSellRecordListForSellerAction" class="sellRecordAction" method="openSellRecordListForSeller">
			<result name="success">/sellrecord/sellrecordlistforseller.jsp</result>
			<result name="input">/sellrecord/sellrecordlistforseller.jsp</result>
		</action>
		<action name="openSellRecordListForCustomerAction" class="sellRecordAction" method="openSellRecordListForCustomer">
			<result name="success">/sellrecord/sellrecordlistforcustomer.jsp</result>
			<result name="input">/sellrecord/sellrecordlistforcustomer.jsp</result>
		</action>
		<action name="openViewSellRecordAction" class="sellRecordAction" method="openViewSellRecord">
			<result name="success">/sellrecord/sellrecordinfo.jsp</result>
			<result name="input">/sellrecord/sellrecordinfo.jsp</result>
		</action>
		<action name="openAddSellRecordForSellerAction" class="sellRecordAction" method="openAddSellRecordForSeller">
			<result name="success">/sellrecord/sellrecordinfoforseller.jsp</result>
			<result name="input">/sellrecord/sellrecordinfoforseller.jsp</result>
		</action>
		<action name="openModifySellRecordForSellerAction" class="sellRecordAction" method="openModifySellRecordForSeller">
			<result name="success">/sellrecord/sellrecordinfoforseller.jsp</result>
			<result name="input">/sellrecord/sellrecordinfoforseller.jsp</result>
		</action>
		<action name="addSellRecordAction" class="sellRecordAction" method="addSellRecordForSeller">
			<result name="success">/sellrecord/sellrecordinfoforseller.jsp</result>
			<result name="input">/sellrecord/sellrecordinfoforseller.jsp</result>
		</action>

		<action name="deleteSellRecordAction" class="sellRecordAction" method="deleteSellRecord"/>
		
		<action name="openAddSellRecordDetialAction" class="sellRecordAction" method="openAddSellRecordDetial">
			<result name="success">/sellrecord/sellrecorddetial.jsp</result>
			<result name="input">/sellrecord/sellrecorddetial.jsp</result>
		</action>
		
		<action name="deleteSellRecordDetialsAction" class="sellRecordAction" method="deleteSellRecordDetials"/>
		
		<action name="showGroupTreeForSellRecordAction" class="sellRecordAction"  method="showGroupTreeForSellRecord">
			<result name="group-tree-for-sell-record">/sellrecord/group-tree-for-sell-record.jsp</result>
		</action>

		<action name="openSellRecordListForOneCustomerAction" class="sellRecordAction" method="openSellRecordListForOneCustomer">
			<result name="success">/sellrecord/sellrecordlistforonecustomer.jsp</result>
			<result name="input">/sellrecord/sellrecordlistforonecustomer.jsp</result>
		</action>
		
		
		<action name="openAddSellRecordForOneCustomerAction" class="sellRecordAction" method="openAddSellRecordForOneCustomer">
			<result name="success">/sellrecord/sellrecordinfoforonecustomer.jsp</result>
			<result name="input">/sellrecord/sellrecordinfoforonecustomer.jsp</result>
		</action>
		<action name="openModifySellRecordForOneCustomerAction" class="sellRecordAction" method="openModifySellRecordForOneCustomer">
			<result name="success">/sellrecord/sellrecordinfoforonecustomer.jsp</result>
			<result name="input">/sellrecord/sellrecordinfoforonecustomer.jsp</result>
		</action>
		<action name="addSellRecordForForOneCustomerAction" class="sellRecordAction" method="addSellRecordForForOneCustomer">
			<result name="success">/sellrecord/sellrecordinfoforonecustomer.jsp</result>
			<result name="input">/sellrecord/sellrecordinfoforonecustomer.jsp</result>
		</action>
		
		
		<action name="openSellRecordListForCustomerViewAction" class="sellRecordAction" method="openSellRecordListForCustomerView">
			<result name="success">/sellrecord/sellrecordlistforcustomerview.jsp</result>
			<result name="input">/sellrecord/sellrecordlistforcustomerview.jsp</result>
		</action>
		
		<action name="openSellRecordListForOneCustomerViewAction" class="sellRecordAction" method="openSellRecordListForOneCustomerView">
			<result name="success">/sellrecord/sellrecordlistforonecustomerview.jsp</result>
			<result name="input">/sellrecord/sellrecordlistforonecustomerview.jsp</result>
		</action>
		<action name="openModifySellRecordForOneCustomerViewAction" class="sellRecordAction" method="openModifySellRecordForOneCustomerView">
			<result name="success">/sellrecord/sellrecordinfoforonecustomerview.jsp</result>
			<result name="input">/sellrecord/sellrecordinfoforonecustomerview.jsp</result>
		</action>
	</package>
	<package name="sellRecordAjax" extends="json-default" namespace="/">
		<action name="getGoodsInfoForAjaxAction" class="sellRecordAction" method="getGoodsInfoForAjax">
            <result type="json" >goodsInfoBase</result>
        </action>
	</package>
