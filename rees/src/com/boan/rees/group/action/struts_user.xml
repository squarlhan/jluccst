<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">	
<struts>
     <!-- 用户Action -->
    <package name="group_user_action" extends="struts-default">
    	<action name="userAction" class="userAction">
    		<result name="show-user">/systemmanage/userinfo.jsp</result>
    		<result name="show-list">/systemmanage/userlist.jsp</result>
    	</action>
		<action name="saveOrUpdateUserAction" class="userAction" method="saveOrUpdateUser">
    		<result name="success">/common/jumppage.jsp</result>
    		<result name="error">/common/messagenoclose.jsp</result>
    	</action>
    	<action name="deleteUserAction" class="userAction" method="deleteUser">
    		<result name="success">/sysmanage/userlist.jsp</result>
    	</action>
    	<action name="showInputUserAction" class="userAction" method="showInputUser">
    		<result name="success">/sysmanage/userinfo.jsp</result>
    	</action>
    	<action name="showMyInfoAction" class="userAction" method="showMyInfo">
    		<result name="success">/sysmanage/myinfo.jsp</result>
    	</action>
    	<action name="saveOrUpdateMyInfoAction" class="userAction" method="saveOrUpdateMyInfo">
    		<result name="success">/common/messagenoclose.jsp</result>
    	</action>
    </package>
</struts>