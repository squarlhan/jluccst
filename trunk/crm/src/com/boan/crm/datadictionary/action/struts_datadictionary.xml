<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">	
<struts>
    <!-- 客户类别Action -->
	<package name="customerType" extends="struts-default" namespace="/datadictionary">
		<action name="customertypelist" class="customerTypeAction" method="customerTypeList">
			<result name="success">/datadictionary/customer_type_list.jsp</result>
			<result name="input">/datadictionary/customer_type_list.jsp</result>
		</action>
		<action name="customertype" class="customerTypeAction" method="customerTypeInfo">
			<result name="success">/datadictionary/customer_type_info.jsp</result>
			<result name="input">/datadictionary/customer_type_info.jsp</result>
		</action>
		<action name="savecustomertype" class="customerTypeAction" method="saveCustomerType">
			<result name="success" type="chain">
             	<param name="actionName">customertype</param>
            </result>
		</action>
    	<action name="delcustomertype" class="customerTypeAction" method="deleteCustomerType">
    		<result name="success" type="chain">
             	<param name="actionName">customertypelist</param>
            </result>
    	</action>
    	<action name="customertypesort" class="customerTypeAction" method="customerTypeSort">
    		<result name="success">/datadictionary/customer_type_sort.jsp</result>
			<result name="input">/datadictionary/customer_type_sort.jsp</result>
    	</action>
    	<action name="savecustomertypesort" class="customerTypeAction" method="saveCustomerTypeSort">
    		<result name="success" type="chain">
             	<param name="actionName">customertypesort</param>
            </result>
    	</action>
	</package>
</struts>