<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <include file="struts-default.xml"/>
	<constant name="struts.i18n.encoding" value="UTF-8"/>
	<constant name="struts.objectFactory" value="spring" />  
	<package name="struts2" extends="struts-default" namespace="/">
		<action name="useraction" class="userAction">
		   <result name="success">adminsuccess.jsp</result>
			<result name="loginsuccess">findreason.jsp</result>
			<result name="loginerror">/userloginerror.jsp</result>
			<result name="searcherror">searcherror.jsp</result>
			<result name="addnullerror">/addnullerror.jsp</result>
			<result name="adderror">/useradderror.jsp</result>
			<result name="altersuccess">userupdatesuccess.jsp</result>
			<result name="altererror">useraltererror.jsp</result>
			<result name="nullpass">usernullpass.jsp</result>
		</action>
		
		
	   
		
	    <action name="ruleaction" class="ruleAction">
			<result name="success">ruleadd.jsp</result>
			
		</action>
		
		
		<action name="adminaction" class="adminAction"  >
			<result name="success">/adminsuccess.jsp</result>
			<result name="altersuccess">updatesuccess.jsp</result>
			<result name="altererror">altererror.jsp</result>
			<result name="nullpass">nullpass.jsp</result>
			<result name="loginerror">/loginerror.jsp</result>
			
			
		</action>
	</package>

</struts>
